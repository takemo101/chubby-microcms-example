{
  "name": "takemo101/chubby-skeleton",
  "type": "project",
  "description": "This is Chubby's project.",
  "keywords": ["php", "slim", "chubbt", "framework"],
  "authors": [
    {
      "name": "takemo101",
      "email": "takemo101@gmail.com"
    }
  ],
  "homepage": "https://github.com/takemo101/chubby",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.8",
    "latte/latte": "^3.0",
    "microcmsio/microcms-php-sdk": "^1.0",
    "takemo101/chubby": "^0.0.5"
  },
  "require-dev": {
    "symfony/var-dumper": "^6.3"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
